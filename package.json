{
  "name": "req-mindmap",
  "private": false,
  "version": "0.3.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "stanah",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "src/**/*.css",
    "src/test/setup.ts"
  ],
  "scripts": {
    "build": "pnpm run build:vscode",
    "build:lib": "vite build --config vite.config.lib.ts",
    "prepublishOnly": "pnpm run build:lib",
    "build:vscode": "vite build --config vite.config.vscode.ts && rm -rf extension/webview && mkdir -p extension/webview && cp -r dist/webview/* extension/webview/ && cd extension && pnpm run compile",
    "build:vscode-package": "./scripts/build-vscode.sh",
    "lint": "eslint .",
    "test": "pnpm test:run && pnpm test:vscode:run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:vscode": "cd extension && pnpm test",
    "test:vscode:run": "cd extension && pnpm test:run",
    "test:vscode:ui": "cd extension && pnpm test:ui"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-dom": ">=18 <20",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "d3": "^7.9.0",
    "js-yaml": "^4.1.0",
    "yaml": "^2.8.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "d3": "^7.9.0",
    "js-yaml": "^4.1.0",
    "yaml": "^2.8.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.7",
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3": "^7.4.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "json-schema-to-typescript": "^15.0.4",
    "monaco-editor": "^0.52.2",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
